//Problem statement:- Write a code to show pass by value.

#include <iostream>
using namespace std;
 void swap(int x, int y){
        int temp=x;
        x=y;
        y=temp;
        return ;
    }

int main() {
    int a=5; int b=2;
    swap(a,b);
    cout<<"a is:"<<a<<endl;
    
    cout<<"b is:"<<b<<endl;

    return 0;
}



/*Output:-

a is:5
b is:2
